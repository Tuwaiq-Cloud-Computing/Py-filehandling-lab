    "def split_text_by_comma(file_path):\n",
    "    with open(file_path, 'r') as f:\n",
    "        text = f.read()\n",
    "    parts = text.split(',')\n",
    "    for i, part in enumerate(parts):\n",
    "        with open(f'part_{i}.txt', 'w') as f:\n",
    "            f.write(part)\n"
   ]
  },
